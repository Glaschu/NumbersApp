<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jamesglasgow.numbersapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DistanceBetweenActivity" />
        <activity android:name=".TaxCalculatorActivity" />
        <activity android:name=".FactorialFinderActivity" />
        <activity android:name=".HappyNumberActivity" />
        <activity android:name=".CoinFlipActivity" />
        <activity android:name=".FindPiAndEActivity" />
        <activity android:name=".FibonacciActivity" />
        <activity android:name=".PrimeFactorizationActivity" />
        <activity android:name=".NextPrimeActivity" />
        <activity android:name=".MorgageCalculatorActivity" />
        <activity android:name=".ChangeReturnActivity"></activity>
    </application>

</manifest>